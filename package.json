		"typescript": "^4.9.5",
		"vite": "^4.1.1",
		"vitest": "^0.28.4"
	},
	"type": "module",
	"dependencies": {
		"@zptxdev/zptx-lib": "^1.0.10",
		"patch-package": "^6.5.0",
		"socket.io-client": "^4.5.4",
		"svelte-toasts": "^1.1.2"
	}
